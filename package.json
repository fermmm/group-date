{
   "name": "poly-dates-server",
   "version": "0.1.0",
   "description": "Poly Dates Server",
   "main": "build/index.js",
   "author": "Fernando Medina",
   "license": "ISC",
   "private": true,
   "scripts": {
      "clean": "rimraf ./build",
      "prebuild": "npm run clean",
      "build": "tsc",
      "start": "node --inspect=localhost:5858 --nolazy -r ts-node/register src/index.ts",
      "dev": "gremlin-server.sh restart && nodemon --watch",
      "dev-win": "gremlin-server.bat restart && nodemon --watch",
      "test": "jest",
      "test:coverage": "jest --coverage",
      "test:watch": "jest --watch",
      "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "style": "prettier --list-different \"src/**/*.ts\" \"test/**/*.ts\"",
      "lint": "tslint \"src/**/*.ts\"",
      "install-visualizer": "git submodule init && git submodule update && cd gremlin-visualizer && npm install",
      "visualizer": "cd gremlin-visualizer && npm start"
   },
   "devDependencies": {
      "@types/gremlin": "^3.4.3",
      "@types/jest": "^23.3.2",
      "@types/koa": "^2.11.1",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-route": "^3.2.4",
      "husky": "^1.1.0",
      "jest": "^24.9.0",
      "nodemon": "^2.0.2",
      "prettier": "^1.19.1",
      "rimraf": "^2.6.2",
      "ts-jest": "^23.10.3",
      "ts-node": "^7.0.1",
      "tslint": "^5.11.0",
      "tslint-config-prettier": "^1.15.0",
      "typescript": "^3.7.5"
   },
   "dependencies": {
      "axios": "^0.19.2",
      "babel-jest": "^24.9.0",
      "dotenv": "^8.2.0",
      "fastest-validator": "^1.0.2",
      "gremlin": "^3.4.4",
      "koa": "^2.11.0",
      "koa-bodyparser": "^4.2.1",
      "koa-route": "^3.2.0",
      "moment": "^2.24.0",
      "ora": "^4.0.3"
   },
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "testMatch": [
         "**/test/**/*.test.ts"
      ]
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run style",
         "pre-push": "npm run lint"
      }
   }
}
